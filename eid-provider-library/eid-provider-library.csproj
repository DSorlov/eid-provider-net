    <None Remove="resources\certs\frejaeid_test.pfx" />
    <None Remove="resources\certs\frejaeid_test_2LQIrINOzwWAVDhoYybqUcXXmVs.jwt" />
    <None Remove="resources\certs\frejaeid_test_HwMHK_gb3_iuNF1advMtlG0-fUs.jwt" />
